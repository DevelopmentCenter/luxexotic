            <span class="fallback-text">{{ 'cart.general.close_cart' | t }}</span>
          </button>
        </div>
        <div class="drawer__title">{{ 'cart.general.title' | t }}</div>
        
      </div>
    </div>
    <div class="drawer__inner">
      <div id="CartContainer" class="drawer__cart"></div>
    </div>
  </div>

