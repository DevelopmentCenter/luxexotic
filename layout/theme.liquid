  <!-- ROIHunterEasy BodySection start -->{% comment pluginspeed replaced-js DON'T CHANGE %}{{ 'roi_hunter_easy_body_section.js' | asset_url | script_tag }}{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'roi_hunter_easy_body_section.js', tag: 'script', liquid_tag: true -%}<!-- ROIHunterEasy BodySection end -->
	
