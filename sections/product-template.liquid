<script>
  jQuery(document).ready(function($){
    var sl = $('.topBarProductFixed');
    $(window).scroll(function() {
      if ($(this).scrollTop() > $('.addToCartPos').offset().top) {
        sl.addClass('active');
      }
      else {
        sl.removeClass('active');
      }
    });

  });
</script>
